{
  "manifest_version": 3,
  "name": "Weather Extension",
  "version": "1.0",
  "description": "A simple weather extension for Chrome",
  "permissions": [
    "geolocation"
  ],
  "host_permissions": [
    "https://api.openweathermap.org/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
